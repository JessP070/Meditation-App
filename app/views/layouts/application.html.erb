<!DOCTYPE html>
<html>
  <head>
    <title>MeditationApp</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <div>
  <%= link_to "Log Out", logout_path, method: :delete %>
  <%= link_to "Add Meditation", new_meditation_path %>
  <%= link_to "All Meditations", meditations_path %>
  <%= link_to "View All Techniques", techniques_path %>


  </div>

  <body>
    <%= yield %>
  </body>
</html>
